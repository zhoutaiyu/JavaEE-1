<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.qdgxy.sss.mapper.UserCustomMapper">

    <select id="findUserByUsername" resultType="UserCustom" parameterType="String">
        select *
        from user
        where username=#{username}
    </select>

</mapper>